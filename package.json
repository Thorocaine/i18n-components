{
	"name": "i18n-components",
	"version": "1.0.6",
	"description": "I want to try make an easy to use i18n components for react, using tagged template literals.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"@storybook/react": "^5.3.14",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.1",
		"@testing-library/user-event": "^7.2.1",
		"react-scripts": "^3.4.0",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.2"
	},
	"peerDependencies": {
		"react": "^16.10"
	},
	"scripts": {
		"storybook": "start-storybook",
		"build": "rimraf dist && tsc",
		"test": "react-scripts test"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"brace-style": [
				2,
				"allman",
				{
					"allowSingleLine": true
				}
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Thorocaine/i18n-components.git"
	},
	"keywords": [
		"i18n",
		"react"
	],
	"author": "Jonathan Peel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Thorocaine/i18n-components/issues"
	},
	"homepage": "https://github.com/Thorocaine/i18n-components#readme",
	"files": ["dist"]
}
